{
  "name": "chargev-db-cloudkit-connector",
  "version": "1.5.1",
  "description": "Connects the chargEV CloudKit Backend with the chargev-db Database",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "prestart": "tsc",
    "build": "tsc ; cp -a cloudkit/eckey.pem build/cloudkit/eckey.pem ; cp -a strings build/",
    "ts-node": "ts-node",
    "postinstall": "npm run install-cloudkit-js ; npm run create-cloudkit-key ; npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-cloudkit-js": "mkdir -p cloudkit/vendor ; curl -qs https://cdn.apple-cloudkit.com/ck/1/cloudkit.js > cloudkit/vendor/cloudkit.js",
    "create-cloudkit-key": "test -s cloudkit/eckey.pem || echo \"$CLOUDKIT_PRIVATE_KEY_FILE\" > cloudkit/eckey.pem"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remuslazar/chargev-db-cloudkit-connector.git"
  },
  "keywords": [
    "chargev",
    "chargev-db",
    "evplugfinder"
  ],
  "author": "Remus Lazar <remus@lazar.info>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/remuslazar/chargev-db-cloudkit-connector/issues"
  },
  "homepage": "https://github.com/remuslazar/chargev-db-cloudkit-connector#readme",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "html-entities": "^1.2.1",
    "node-fetch": "^2.1.2",
    "node-object-hash": "^1.3.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  }
}
